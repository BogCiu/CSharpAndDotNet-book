<Window x:Class="LoopThroughXmlDocument.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoopThroughXmlDocument"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <TextBlock Name="textBlockResults" HorizontalAlignment="Left" Margin="10,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Height="414" Width="695"/>
        </ScrollViewer>
        <Button Name="buttonLoop" Content="Loop" HorizontalAlignment="Left" Margin="710,48,0,0" VerticalAlignment="Top" Width="80" Click="buttonLoop_Click"/>
        <Button Name="buttonCreateNode" Content="Create" HorizontalAlignment="Left" Margin="710,78,0,0" VerticalAlignment="Top" Width="80" Click="buttonCreateNode_Click"/>
        <Button Name="buttonDeleteNode" Content="Delete" HorizontalAlignment="Left" Margin="710,109,0,0" VerticalAlignment="Top" Width="80" Click="buttonDeleteNode_Click"/>

    </Grid>
</Window>
